{
	"_import": {
		"system": "frizz.io/system",
		"validators": "frizz.io/validators"
	},
	"_type": "system.Type",
	"Validators": [
		{
			"_type": "validators.Struct",
			"_value": {
				"String": [
					{
						"_type": "validators.Regex",
						"Regex": "^foo.*$"
					}
				],
				"Int": [
					{
						"_type": "validators.GreaterThan",
						"_value": 2
					}
				]
			}
		}
	]
}